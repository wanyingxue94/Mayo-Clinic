<h1>Doctor log in</h1>
	<%= form_tag do %>
	<div class="form-group">
		<%= label_tag :email %>
		<%= text_field_tag :email, "", :class=>'form-control' %>
	</div>
	
	<div class="form-group">
		<%= label_tag :password %>
		<%= password_field_tag :password, "", :class=>'form-control' %>
	</div> 
		<%= submit_tag "Log in", :class=>'btn btn-primary'%>
		
	<% end %>